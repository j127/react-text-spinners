{
    "name": "react-text-spinners",
    "version": "3.0.1",
    "description": "text-spinners as a React component",
    "author": "j127",
    "license": "MIT",
    "repository": "https://github.com/j127/react-text-spinners",
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "build": "rollup -c",
        "start": "rollup -c -w",
        "examples:serve": "parcel serve example/index.html",
        "examples:build": "parcel build --out-dir=build example/index.html",
        "examples:deploy": "npm run examples:build && netlify deploy --dir=build --prod",
        "publish:npm": "rm -rf dist && npm run build && npm publish"
    },
    "private": false,
    "peerDependencies": {
        "react": "^18.0.0",
        "react-dom": "^18.0.0"
    },
    "keywords": [
        "react",
        "spinners",
        "css",
        "text spinners",
        "typescript"
    ],
    "devDependencies": {
        "@types/react": "^18.0.19",
        "@types/react-dom": "^18.0.6",
        "babel-core": "^6.26.3",
        "babel-runtime": "^6.26.0",
        "bulma": "^0.9.2",
        "lodash": "^4.17.21",
        "netlify-cli": "^11.7.1",
        "parcel-bundler": "^1.12.5",
        "prettier": "^2.0.4",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "rollup": "^2.79.0",
        "rollup-plugin-sass": "^1.2.2",
        "rollup-plugin-typescript2": "^0.30.0",
        "sass": "^1.54.9",
        "typescript": "^4.8.3"
    },
    "files": [
        "dist"
    ],
    "main": "dist/index.js",
    "dependencies": {
        "text-spinners": "^1.0.5"
    }
}
